<h2 class="text-danger">Historial</h2>
    {{#if products.length > 0}}
        <div class="table-responsive">
            <table class="table table-dark">
                <thead>
                    <tr>
                        <th class="col">#</th>
                        <th class="col">Nombre del producto</th>
                        <th class="col">Precio </th>
                        <th class="col">imagen </th>
                    </tr>
                </thead>
                <tbody>
                    {{#each products}}
                        <tr>
                            <th scope="row" >{{ products.indexOf(this)+1 }}</th>
                            <td> {{this.productName}} </td>
                            <td> {{this.productPrice }}</td>
                            <td><img src={{this.product.productImg}} > </td>
                        </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    {{else}}
        <h3 class="text-danger bg-red">No se encontraron productos</h3>
    {{/if}}