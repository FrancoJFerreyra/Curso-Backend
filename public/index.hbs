<div>
    <form id="chatForm">
        <div class="form-group container">
            <label for="email"><b>Ingrese su email</b></label>
            <input type="email" class="form-control container" name="id" id="email" placeholder="abcd@abc.com" required>
            <label for="name"><b>Ingrese su nombre</b></label>
            <input type="text" class="form-control container" name="name" id="name" placeholder="Juan" required></input>
            <label for="lastname"><b>Ingrese su apellido</b></label>
            <input type="text" class="form-control container" name="lastname" id="lastname" placeholder="Perez" required></input>
            <label for="age"><b>Ingrese su edad</b></label>
            <input type="number" class="form-control container" name="age" id="age" placeholder="19" required></input>
            <label for="alias"><b>Ingrese su alias</b></label>
            <input type="text" class="form-control container" name="alias" id="alias"
                placeholder="juanPerez212" required></input>
            <label for="avatar"><b>Ingrese su avatar</b></label>
            <input type="url" class="form-control container" name="avatar" id="avatar"
                placeholder="Url de la imagen" required></input>
            <label for="message"><b>Escriba su mensaje</b></label>
            <input type="text" name="message" id="inputChat" class="mt-3" required>
            <div class="text-center">
                <input type="submit" class="btn btn-succes mt-3 mb-5 btn-secondary"></button>
            </div>
        </div>
    </form>
    <div id="chatMsj">
    </div>
</div>
<script src="./socket.io/socket.io.min.js"></script>
<script src="./client.js"></script>