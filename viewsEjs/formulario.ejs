
    <h1><b>Ingrese el producto nuevo</b></h1>
    <br>
    <form action="/products" method="post">
        <div class="form-group container">
            <label for="productName">Nombre del producto</label>
            <input type="text" class="form-control container" name="productName" id="productName" placeholder="ej:Queso">
        </div>
        <div class="form-group container">
            <label for="productPrice">Precio del producto</label>
            <input type="number" class="form-control container" name="productPrice" id="productPrice" placeholder="ej:$1234">
        </div>
        <div class="form-group container">
            <label for="productImg">Foto del producto</label>
            <input type="url" class="form-control container" name="productImg" id="productImg" placeholder="Url de la imagen">
        </div>
        <button class="btn btn-succes mt-3 mb-5 btn-secondary">Enviar</button>
    </form>
